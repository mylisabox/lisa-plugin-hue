{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": [
        "google"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "require-jsdoc": 0,
        "valid-jsdoc": 0,
        "max-len": [2, {
            "code": 120,
            "tabWidth": 2,
            "ignoreUrls": true,
            "ignorePattern": "goog\\.(module|require)"
        }]
    }
}
